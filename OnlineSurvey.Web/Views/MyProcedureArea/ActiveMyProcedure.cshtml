@model OnlineSurvey.ViewModel.UserProcedureViewModel

@{
    ViewBag.Title = "ActiveMyProcedure";
    Layout = "~/Views/Shared/_UserDashboard.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-10 offset-2">
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-horizontal">
                    <h4>UserProcedureViewModel</h4>
                    <hr />
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })


                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            <input type="submit" value="Active my procedure" class="btn btn-default btn-rounded" />
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>



